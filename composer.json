{
    "name": "yii-test2",
    "description": "Test work",
    "keywords": [],
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sergebezborodov/elog2.git"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "dev-master",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "sergebezborodov/elog2": "master",
        "danielstjules/stringy": "dev-master",
        "guzzlehttp/guzzle": "~5.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "yiisoft/yii2-sphinx": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "writable": [
            "apps/backend/runtime",
            "apps/backend/web/assets",

            "apps/console/runtime",
            "apps/console/migrations",

            "apps/frontend/runtime",
            "apps/frontend/web/assets"
        ]
    }
}
