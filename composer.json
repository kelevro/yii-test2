{
    "name": "malmo/engine",
    "description": "Malmo Engine on the Yii2",
    "keywords": ["malmo", "framework", "advanced", "application template"],
    "homepage": "http://www.stanton-lab.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sergebezborodov/nested-set-behavior-2.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sergebezborodov/elog2.git"
        },
        {
            "type" : "package",
            "package"  : {
                "name"    : "libraries/simplehtmldom",
                "version" : "1.5",
                "dist"  : {
                    "type"  : "zip",
                    "url"  : "http://downloads.sourceforge.net/project/simplehtmldom/simplehtmldom/1.5/simplehtmldom_1_5.zip?r=http%3A%2F%2Fsourceforge.net%2Fprojects%2Fsimplehtmldom%2F&ts=1342886637&use_mirror=kent"
                },
                "autoload": {
                    "files"    : ["simple_html_dom.php"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "dev-master",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "sergebezborodov/elog2": "master",
        "sergebezborodov/nested-set-behavior-2": "dev-master",
        "danielstjules/stringy": "dev-master",
        "guzzlehttp/guzzle": "~5.0",
        "ext-imagick": "*",
        "phpoffice/phpexcel": "dev-develop"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "writable": [
            "apps/backend/runtime",
            "apps/backend/web/assets",

            "apps/console/runtime",
            "apps/console/migrations",

            "apps/frontend/runtime",
            "apps/frontend/web/assets"
        ]
    }
}
